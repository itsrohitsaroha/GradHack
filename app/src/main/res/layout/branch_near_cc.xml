<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/coordinatorLayout3"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".BranchNearCCActivity">


    <ImageView
        android:id="@+id/branch_near_cc"
        android:layout_width="356dp"
        android:layout_height="507dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.428"
        app:srcCompat="@drawable/branch_near_cc" />

    <ImageView
        android:id="@+id/shake"
        android:layout_width="362dp"
        android:layout_height="73dp"
        android:scaleType="fitXY"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.166"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/branch_near_cc"
        app:srcCompat="@drawable/shake" />

    <Button
        android:id="@+id/next_visit_button"
        android:layout_width="313dp"
        android:layout_height="415dp"
        android:layout_marginStart="21dp"
        android:layout_marginLeft="21dp"
        android:background="@null"
        app:layout_constraintBottom_toTopOf="@+id/shake"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/branch_near_cc"
        app:layout_constraintTop_toTopOf="@+id/branch_near_cc" />

</androidx.constraintlayout.widget.ConstraintLayout>